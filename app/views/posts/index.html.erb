<h1>投稿一覧表</h1>

<table>
  <tr>
    <th>内容</th>
  </tr>

  <% @post.each do |post| %>
    <tr>
      <td><%= post.content %></td>
      <td><%= link_to "投稿を確認する", post_path(post.id) %></td>
      <td><%= link_to "投稿を編集する", edit_post_path(post.id) %></td>
      <td><%= link_to "投稿を削除する", post_path(post.id), method: :delete %></td>
    </tr>
  <% end %>
<table>
<%= link_to "新しく投稿する", new_post_path %>